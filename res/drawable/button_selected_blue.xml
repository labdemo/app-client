<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape>
            <gradient android:startColor="@color/loginbuttonsel" android:endColor="@color/loginbuttonsel"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
    
	<item >
        <shape>
            <gradient android:startColor="@color/loginbuttonnormal" android:endColor="@color/loginbuttonnormal"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</selector>
